{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Auth0": {
    "Authority": "https://<CLIENT APP DOMAIN URL>",
    "Domain": "<CLIENT APP DOMAIN URL>",
    "ClientId": "<CLIENT ID>",
    "ClientSecret": "<CLIENT SECRET>", //Move this to secrets
    "Audience": "<API AUDIENCE URL>"
  },
  "YourApiBaseUrl": "https://localhost:<API PORT>",
  "AllowedHosts": "*"
}
